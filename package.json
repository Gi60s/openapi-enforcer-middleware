{
  "name": "openapi-enforcer-middleware",
  "version": "2.2.0",
  "description": "An express middleware that makes it easy to write web services that follow an Open API specification.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "build:clean": "rimraf dist/ && npm run build",
    "coverage": "nyc npm run test",
    "docs:build": "markdown-docs build docs-src docs",
    "docs:dev": "markdown-docs dev docs-src",
    "test": "mocha test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byu-oit/openapi-enforcer-middleware.git"
  },
  "keywords": [
    "openapi",
    "swagger",
    "express",
    "middleware",
    "enforcer",
    "validate",
    "parse",
    "format",
    "openapi-enforcer"
  ],
  "author": "James Speirs <james.speirs@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/byu-oit/openapi-enforcer-middleware/issues"
  },
  "homepage": "https://middleware.openapi-enforcer.com/",
  "dependencies": {
    "debug": "^3.2.7"
  },
  "peerDependencies": {
    "express": ">= 4.0.0",
    "openapi-enforcer": ">= 1.2.0"
  },
  "devDependencies": {
    "@gi60s/markdown-docs": "0.0.10",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.37",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "express": "^4.17.1",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "openapi-enforcer": "^1.13.1",
    "redoc": "^2.0.0-rc.48",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "standard": "^12.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
